mso-ansi-language:EN-US'>&lt;?php<br>
$servername =&nbsp;&quot;localhost&quot;;<br>
$username =&nbsp;&quot;username&quot;;<br>
$password =&nbsp;&quot;password&quot;;<br>
$dbname =&nbsp;&quot;myDB&quot;;<br>
// Create connection<br>
$conn = mysqli_connect($servername, $username, $password, $dbname);<br>
// Check connection<br>
if&nbsp;(!$conn)&nbsp;{<br>
&nbsp;&nbsp;&nbsp;&nbsp;die(&quot;Connection failed: &quot;&nbsp;.
mysqli_connect_error());<br>
}<br>
// sql to create table<br>
$sql =&nbsp;&quot;CREATE TABLE MyGuests (<br>
id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY,&nbsp;<br>
firstname VARCHAR(30) NOT NULL,<br>
lastname VARCHAR(30) NOT NULL,<br>
email VARCHAR(50),<br>
reg_date TIMESTAMP<br>
)&quot;;<br>
<span style='background:yellow;mso-highlight:yellow'>if&nbsp;(mysqli_query($conn,
$sql)) {</span><br>
&nbsp; &nbsp;&nbsp;echo&nbsp;&quot;Table MyGuests created successfully&quot;;<br>
}&nbsp;else&nbsp;{<br>
&nbsp;&nbsp;&nbsp;&nbsp;echo&nbsp;&quot;Error creating table: &quot;&nbsp;.
mysqli_error($conn);<br>
}<br>
mysqli_close($conn);<br>
?&gt;<o:p></o:p></span></i></p>

<h4><span lang=EN-US style='color:#595959;mso-themecolor:text2;mso-themetint:
